<%- include('header') -%>
<div class = "box" id="heading">
    <h1><%=listtitle%></h1>
</div>
<div class="box">
    <%for (var i =1;i<newlistitems.length;i++){ %>
    <div class="item">
        <input type="checkbox">
        <p><%=newlistitems[i]%></p>
    </div>    
    <%}%>
    <form action="/" method="POST">
        <input type="text" name="tasklist" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%= listtitle %>  >+</button>
    </form>
</div>
<%- include('footer') -%>